{
  "name": "node-red-spreadsheet-template",
  "version": "1.1.1",
  "description": "A Node-RED node for rendering spreadsheets using a spreadsheet template.",
  "author": {
    "name": "AllanOricil",
    "email": "allanoricil@duck.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AllanOricil/node-red-spreadsheet-template"
  },
  "bugs": {
    "url": "https://github.com/AllanOricil/node-red-spreadsheet-template/issues"
  },
  "homepage": "https://github.com/AllanOricil/node-red-spreadsheet-template#readme",
  "license": "MIT",
  "keywords": [
    "node-red",
    "nrg"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "build": "nrg build",
    "build:dev": "nrg build -e dev",
    "watch": "nrg dev -w -o",
    "watch:debug": "nrg dev -w -o -d",
    "start": "nrg dev -o",
    "start:debug": "nrg dev -o -d",
    "start:prod": "nrg dev -o -d -e prod",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier **/*.{js,json,html} --ignore-unknown",
    "format:fix": "prettier **/*.{js,json,html} --write --ignore-unknown",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "**/*.{json, html}": "prettier --write --ignore-unknown",
    "**/*.{js}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "node-red": {
    "version": ">=3.1",
    "nodes": {
      "nodes": "dist/index.js"
    }
  },
  "dependencies": {
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@allanoricil/nrg-cli": "2.0.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "12.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.1.5",
    "lint-staged": "15.2.0",
    "prettier": "^3.2.5",
    "semantic-release": "23.1.1"
  }
}
